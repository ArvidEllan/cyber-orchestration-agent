{
  "name": "@security/infra-agent",
  "version": "1.0.0",
  "description": "Infrastructure Security Review Agent for AWS cloud infrastructure",
  "main": "dist/index.js",
  "bin": {
    "infra-agent": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "security",
    "aws",
    "infrastructure",
    "iac",
    "compliance",
    "terraform",
    "cloudformation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^4.0.8",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.930.0",
    "@aws-sdk/client-cloudtrail": "^3.930.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.948.0",
    "@aws-sdk/client-config-service": "^3.930.0",
    "@aws-sdk/client-ec2": "^3.930.0",
    "@aws-sdk/client-eks": "^3.930.0",
    "@aws-sdk/client-guardduty": "^3.948.0",
    "@aws-sdk/client-iam": "^3.930.0",
    "@aws-sdk/client-lambda": "^3.930.0",
    "@aws-sdk/client-organizations": "^3.930.0",
    "@aws-sdk/client-rds": "^3.930.0",
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/client-securityhub": "^3.948.0",
    "@aws-sdk/client-sts": "^3.930.0",
    "@aws-sdk/credential-providers": "^3.930.0",
    "@cdktf/hcl2json": "^0.21.0",
    "hcl2-parser": "^1.0.3",
    "js-yaml": "^4.1.1",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
